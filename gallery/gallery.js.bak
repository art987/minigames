// ç”Ÿæˆéšæœºé¢œè‰²ï¼ˆå¤‡ç”¨ï¼‰
function getRandomColor() {
    const letters = 'BCDEF'; // é¿å…ä½¿ç”¨å¤ªæš—çš„é¢œè‰²
    let color = '#';
    for (let i = 0; i < 6; i++) {
        color += letters[Math.floor(Math.random() * letters.length)];
    }
    return color;
}

// ä¸»å‡½æ•° - åˆå§‹åŒ–ç”»å»Š
function initGallery() {
    // éšè—åŠ è½½çŠ¶æ€
    setTimeout(() => {
        document.getElementById('loading-indicator').style.display = 'none';
        renderFilters();
        renderGallery(famousPaintings);
    }, 800); // æ¨¡æ‹ŸåŠ è½½å»¶è¿Ÿ
}

// åŠ¨æ€ç”Ÿæˆç­›é€‰æŒ‰é’®
function renderFilters() {
    const filtersContainer = document.getElementById('filters-container');
    
    // è·å–æ‰€æœ‰æœä»£å¹¶å»é‡
    const periods = [...new Set(famousPaintings.map(p => p.period))];
    periods.sort(); // æŒ‰æœä»£æ’åº
    
    // åˆ›å»º"å…¨éƒ¨"æŒ‰é’®
    const allBtn = document.createElement('button');
    allBtn.className = 'filter-btn active';
    allBtn.textContent = 'å…¨éƒ¨';
    allBtn.dataset.type = 'all';
    allBtn.addEventListener('click', () => {
        applyFilter('all', null);
    });
    filtersContainer.appendChild(allBtn);
    
    // åˆ›å»ºæœä»£ç­›é€‰æŒ‰é’®
    periods.forEach(period => {
        const btn = document.createElement('button');
        btn.className = 'filter-btn';
        btn.textContent = period;
        btn.dataset.type = 'period';
        btn.dataset.value = period;
        btn.addEventListener('click', () => {
            applyFilter('period', period);
        });
        filtersContainer.appendChild(btn);
    });
}

// åº”ç”¨ç­›é€‰æ¡ä»¶
function applyFilter(type, value) {
    // é‡ç½®æ‰€æœ‰ç­›é€‰æŒ‰é’®æ ·å¼
    document.querySelectorAll('.filter-btn').forEach(btn => {
        btn.classList.remove('active');
    });
    
    // é«˜äº®å½“å‰é€‰ä¸­çš„ç­›é€‰æŒ‰é’®
    if (type === 'all') {
        document.querySelector('.filter-btn[data-type="all"]').classList.add('active');
    } else {
        document.querySelector(`.filter-btn[data-type="${type}"][data-value="${value}"]`).classList.add('active');
    }
    
    // æ ¹æ®ç­›é€‰æ¡ä»¶è¿‡æ»¤æ•°æ®
    let filteredPaintings = famousPaintings;
    if (type === 'period' && value) {
        filteredPaintings = famousPaintings.filter(p => p.period === value);
    }
    
    // é‡æ–°æ¸²æŸ“ç”»å»Š
    renderGallery(filteredPaintings);
}

// åŠ¨æ€ç”Ÿæˆåç”»å¡ç‰‡
function renderGallery(paintings) {
    const galleryContainer = document.getElementById('gallery-container');
    galleryContainer.innerHTML = ''; // æ¸…ç©ºå®¹å™¨
    
    if (paintings.length === 0) {
        // å¦‚æœæ²¡æœ‰åŒ¹é…çš„ä½œå“ï¼Œæ˜¾ç¤ºæç¤ºä¿¡æ¯
        const emptyState = document.createElement('div');
        emptyState.className = 'empty-state';
        emptyState.style.textAlign = 'center';
        emptyState.style.padding = '50px 20px';
        emptyState.style.gridColumn = '1 / -1';
        emptyState.innerHTML = `
            <div style="font-size: 48px; margin-bottom: 15px;">ğŸ“š</div>
            <p style="font-size: 18px; color: #666;">æš‚æ— åŒ¹é…çš„åç”»ä½œå“</p>
        `;
        galleryContainer.appendChild(emptyState);
        return;
    }
    
    // åŠ¨æ€ç”Ÿæˆç”»ä½œå¡ç‰‡
    paintings.forEach(painting => {
        const card = document.createElement('div');
        card.className = 'art-card';
        
        // ä¸ºæ¯ä¸ªå¡ç‰‡æ·»åŠ åŠ¨ç”»å»¶è¿Ÿï¼Œå®ç°é”™è½æœ‰è‡´çš„å‡ºç°æ•ˆæœ
        card.style.animationDelay = `${Math.random() * 0.5}s`;
        
        // å¡ç‰‡å†…å®¹
        card.innerHTML = `
            <div class="art-image">
                <img src="${painting.imageUrl}" alt="${painting.title}">
            </div>
            <div class="art-info">
                <h3 class="art-title">${painting.title}</h3>
                <div class="art-meta">
                    ${painting.period}
                    ${painting.author ? ` Â· ${painting.author}` : ''}
                    ${painting.type ? ` Â· ${painting.type}` : ''}
                </div>
                <p class="art-desc">${painting.description}</p>
                <div class="art-footer">
                    <span class="cover-text">${painting.coverText}</span>
                    <button class="details-btn" onclick="showPaintingDetails(${painting.id})">æŸ¥çœ‹è¯¦æƒ…</button>
                </div>
            </div>
        `;
        
        galleryContainer.appendChild(card);
    });
}

// æ˜¾ç¤ºç”»ä½œè¯¦æƒ…
function showPaintingDetails(paintingId) {
    const painting = famousPaintings.find(p => p.id === paintingId);
    if (!painting) return;
    
    // è·å–è¯¦æƒ…å¼¹çª—
    const modal = document.getElementById('detail-modal');
    const modalContent = document.getElementById('detail-content');
    
    // æ›´æ–°å¼¹çª—å†…å®¹
    modalContent.innerHTML = `
        <div class="detail-image">
            <img src="${painting.imageUrl}" alt="${painting.title}">
        </div>
        <div class="detail-info">
            <h2>${painting.title}</h2>
            <div class="meta">
                ${painting.period}
                ${painting.author ? ` Â· ${painting.author}` : ''}
                ${painting.type ? ` Â· ${painting.type}` : ''}
            </div>
            <div class="description">
                <h3>ä½œå“ä»‹ç»</h3>
                <p>${painting.description}</p>
            </div>
        </div>
    `;
    
    // æ˜¾ç¤ºå¼¹çª—
    modal.style.display = 'block';
    
    // æ·»åŠ å…³é—­å¼¹çª—çš„äº‹ä»¶ç›‘å¬
    const closeBtn = modal.querySelector('.close-btn');
    closeBtn.addEventListener('click', closeModal);
    
    // ç‚¹å‡»å¼¹çª—å¤–éƒ¨å…³é—­
    modal.addEventListener('click', (e) => {
        if (e.target === modal) {
            closeModal();
        }
    });
    
    // é˜²æ­¢ç‚¹å‡»å¼¹çª—å†…å®¹æ—¶å…³é—­
    modal.querySelector('.modal-content').addEventListener('click', (e) => {
        e.stopPropagation();
    });
    
    // æ·»åŠ ESCé”®å…³é—­
    document.addEventListener('keydown', handleEscKey);
    
    function handleEscKey(e) {
        if (e.key === 'Escape') {
            closeModal();
        }
    }
}

// å…³é—­æ¨¡æ€æ¡†
function closeModal() {
    const modal = document.getElementById('detail-modal');
    if (modal) {
        modal.style.display = 'none';
        document.removeEventListener('keydown', handleEscKey);
    }
}

